{
  "manifest_version": 2,

  "name": "Autocomplete On!",
  "description": "Stop pages from disabling autocomplete on fields.",
  "version": "1.0.0",
  "offline_enabled": true,

  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"},

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}